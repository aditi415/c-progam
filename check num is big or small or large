#include <stdio.h>
int main()
{
    int i;
    printf("Enter an integer:");
    scanf("%d",&i);
    if(i<100)
    {
        printf("%d number is small");
    }
    else if(i>100 && i<1000)
    {
        printf("%d number is big number");
    }
    else
    {
        printf("large number");
    }
    return 0;
}
