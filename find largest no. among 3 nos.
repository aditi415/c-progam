#include <stdio.h>
int main()
{      int a,b,c;                                                                       
    printf("Enter an integer:");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c)
       printf("%d is largest number",a);
    else if(b>c && b>a)
       printf("%d is largest number",b);
    else if(c>a && c>b)
        printf("%d is largest number",c);
    return 0;
}
