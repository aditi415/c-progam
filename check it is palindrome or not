#include <stdio.h>
int main()
{
    int n2=n;
    ans=0;
    while(n!=0)
    {
        ans=(ans*10)+(n%10);
        n=n/10;
    }
      printf("ans");
      if(n2==ans)
      {
          printf("It is palidrome");
      }
      else
      {
          printf("not a palidrome");
      }
    return 0;
}
