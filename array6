#include <stdio.h>

int main()
{
    int a,b,quot,rem;
    printf("Enter a and b: ");
    scanf("%d%d",&a,&b);                                                                 
    if(b==0)
        printf("Error");  
    else                
       quot=a/b;
       rem=a%b;
    printf("Quotient:%d\n",quot);
    printf("remainder:%d\n",rem);
  return 0;    
}
       
